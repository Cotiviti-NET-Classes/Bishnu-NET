@model List<Employee>

<h4>Employees</h4>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Gender</th>
            <th scope="col">Join Date</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <th scope="row">@(i+1)</th>
                <td>@Model[i].Name</td>
                <td>@Model[i].Gender</td>
                <td>@Model[i].JoiningDate</td>
            </tr>
        }
    </tbody>
</table>
